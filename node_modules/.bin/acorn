#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/mmazo/Desktop/Master/Info802-archi/Projet/API/node_modules/acorn/bin/node_modules:/mnt/c/Users/mmazo/Desktop/Master/Info802-archi/Projet/API/node_modules/acorn/node_modules:/mnt/c/Users/mmazo/Desktop/Master/Info802-archi/Projet/API/node_modules:/mnt/c/Users/mmazo/Desktop/Master/Info802-archi/Projet/node_modules:/mnt/c/Users/mmazo/Desktop/Master/Info802-archi/node_modules:/mnt/c/Users/mmazo/Desktop/Master/node_modules:/mnt/c/Users/mmazo/Desktop/node_modules:/mnt/c/Users/mmazo/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/mmazo/Desktop/Master/Info802-archi/Projet/API/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/mmazo/Desktop/Master/Info802-archi/Projet/API/node_modules/acorn/bin/node_modules:/mnt/c/Users/mmazo/Desktop/Master/Info802-archi/Projet/API/node_modules/acorn/node_modules:/mnt/c/Users/mmazo/Desktop/Master/Info802-archi/Projet/API/node_modules:/mnt/c/Users/mmazo/Desktop/Master/Info802-archi/Projet/node_modules:/mnt/c/Users/mmazo/Desktop/Master/Info802-archi/node_modules:/mnt/c/Users/mmazo/Desktop/Master/node_modules:/mnt/c/Users/mmazo/Desktop/node_modules:/mnt/c/Users/mmazo/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/mmazo/Desktop/Master/Info802-archi/Projet/API/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
